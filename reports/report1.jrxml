	<queryString>
		<![CDATA[SELECT
     open_palance_date.`date` AS open_palance_date_date,
     open_palance.`id` AS open_palance_id,
     open_palance.`itemid` AS open_palance_itemid,
     open_palance.`quantity` AS open_palance_quantity,
     open_palance.`cost` AS open_palance_cost,
     open_palance.`discount` AS open_palance_discount,
     open_palance.`storeid` AS open_palance_storeid,
     stores.`id` AS stores_id,
     stores.`name_en` AS stores_name_en,
     stores.`name_ar` AS stores_name_ar,
     units.`id` AS units_id,
     units.`name_en` AS units_name_en,
     units.`name_ar` AS units_name_ar,
     items.`id` AS items_id,
     items.`name_en` AS items_name_en,
     items.`name_ar` AS items_name_ar,
     items.`barcode` AS items_barcode,
     items.`disc` AS items_disc,
     items.`unit` AS items_unit,
     items.`intimate_stock` AS items_intimate_stock,
     items.`measurement` AS items_measurement,
     units.`id` AS units_id,
     units.`name_en` AS units_name_en,
     units.`name_ar` AS units_name_ar,
     main_settings.`id` AS main_settings_id,
     main_settings.`name_en` AS main_settings_name_en,
     main_settings.`name_ar` AS main_settings_name_ar,
     main_settings.`address` AS main_settings_address,
     open_palance.`store` AS open_palance_store
FROM
     `stores` stores INNER JOIN `open_palance` open_palance ON stores.`id` = open_palance.`store`
     INNER JOIN `items` items ON open_palance.`itemid` = items.`id`
     LEFT JOIN  units ON items.`unit` = units.`id`,
     `open_palance_date` open_palance_date,
     `main_settings` main_settings
]]>
	</queryString>
