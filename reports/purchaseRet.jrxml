     purchase_ret.`user` AS purchase_ret_user,
     purchase_ret.`gross_total` AS purchase_ret_gross_total,
     purchase_ret.`vat_all` AS purchase_ret_vat_all,
     purchase_ret.`dis_added` AS purchase_ret_dis_added,
     purchase_ret.`all_total` AS purchase_ret_all_total,
     purchase_ret.`paymentType` AS purchase_ret_paymentType,
     purchase_ret.`state` AS purchase_ret_state,
     purchase_ret_items.`id` AS purchase_ret_items_id,
     purchase_ret_items.`itemid` AS purchase_ret_items_itemid,
     purchase_ret_items.`quantity` AS purchase_ret_items_quantity,
     purchase_ret_items.`cost` AS purchase_ret_items_cost,
     purchase_ret_items.`discount` AS purchase_ret_items_discount,
     purchase_ret_items.`tax` AS purchase_ret_items_tax,
     purchase_ret_items.`store` AS purchase_ret_items_store,
     purchase_ret_items.`invoice_No` AS purchase_ret_items_invoice_No,
     purchase_ret_items.`storeid` AS purchase_ret_items_storeid,
     purchase_ret_items.`state` AS purchase_ret_items_state,
     branches.`id` AS branches_id,
     branches.`name_en` AS branches_name_en,
     branches.`storeid` AS branches_storeid,
     branches.`city` AS branches_city,
     branches.`state` AS branches_state,
     branches.`name_ar` AS branches_name_ar,
     suppliers.`id` AS supplier_id,
     suppliers.`name_en` AS suppliers_name_en,
     suppliers.`name_ar` AS suppliers_name_ar,
     suppliers.`default` AS suppliers_default,
     units.`id` AS units_id,
     units.`name_en` AS units_name_en,
     units.`name_ar` AS units_name_ar,
     units.`storeid` AS units_storeid,
     units.`default` AS units_default,
     items.`id` AS items_id,
     items.`name_en` AS items_name_en,
     items.`name_ar` AS items_name_ar,
     items.`catid` AS items_catid,
     items.`barcode` AS items_barcode,
     items.`unit` AS items_unit,
     items.`price` AS items_price,
     items.`discount` AS items_discount,
     items.`intimate_stock` AS items_intimate_stock,
     items.`storeid` AS items_storeid,
     items.`enabled` AS items_enabled,
     items.`measurement` AS items_measurement,
     items.`item_type` AS items_item_type,
     items.`item_thread` AS items_item_thread,
     items.`vat` AS items_vat,
     items.`selt` AS items_selt,
     items.`taxInPrice` AS items_taxInPrice,
     units.`id` AS units_id,
     units.`name_en` AS units_name_en,
     units.`name_ar` AS units_name_ar,
     units.`storeid` AS units_storeid,
     units.`default` AS units_default
FROM
     `purchase_ret` purchase_ret INNER JOIN `branches` branches ON purchase_ret.`branch` = branches.`id`
     INNER JOIN `purchase_ret_items` purchase_ret_items ON purchase_ret.`invoicenumber` = purchase_ret_items.`invoice_No`
     INNER JOIN `suppliers` suppliers ON purchase_ret.`supplier` = suppliers.`id`
     INNER JOIN `items` items ON purchase_ret_items.`itemid` = items.`id`
     left JOIN `units` units ON items.`unit` = units.`id`
WHERE purchase_ret.`invoicenumber` =$P{sale_no}]]>
